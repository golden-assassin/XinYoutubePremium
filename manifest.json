{
  "manifest_version": 3,
  "name": "AdBlock",
  "description": "Automatically removes Youtube ad elements",
  "version": "1.0.7",
  "content_scripts": [
    {
      "js": [
        "scripts/contents.js",
        "scripts/blocker.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ]
}
