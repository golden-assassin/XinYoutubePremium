{
  "manifest_version": 3,
  "name": "XinYoutubePremium",
  "description": "Automatically removes Youtube ad elements",
  "version": "1.0.8",
  "content_scripts": [
    {
      "js": [
        "scripts/contents.js",
        "scripts/blocker.js",
        "scripts/sideview.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ]
}
